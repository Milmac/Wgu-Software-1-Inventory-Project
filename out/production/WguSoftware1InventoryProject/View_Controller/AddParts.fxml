<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="575.0" prefWidth="556.0" stylesheets="@MainScreenStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.AddPartsController">
    <children>
        <VBox alignment="TOP_LEFT" prefHeight="575.0" prefWidth="560.0" spacing="10.0">
            <children>
                <Label fx:id="addPartLabel" contentDisplay="LEFT" text="Add Part">
                    <padding>
                        <Insets bottom="20.0" left=" 20" top="20.0" />
                    </padding>
                </Label>
                <HBox alignment="CENTER_LEFT" prefHeight="40.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <RadioButton fx:id="addPartInhouseRadioButton" mnemonicParsing="false" onAction="#addPartInHouseRadioPushed" selected="true" text="In House" />
                        <RadioButton fx:id="addPartOutsourcedRadioButton" mnemonicParsing="false" onAction="#addPartOutsourcedRadioPushed" text="Outsource" />
                    </children>
                    <padding>
                        <Insets left=" 20" />
                    </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="addPartIDLabel" text="Part ID">
                            <padding>
                                <Insets left=" 20" />
                            </padding>
                        </Label>
                        <TextField fx:id="addPartIDTextField" disable="true" editable="false" promptText="Auto Gen  - ID">
                     <HBox.margin>
                        <Insets left="55.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="addPartNameLabel" text="Part Name">
                            <padding>
                                <Insets left=" 20" />
                            </padding>
                        </Label>
                        <TextField fx:id="addPartNameTextField" promptText="Part Name">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="addPartInvLevelLabel" text="Inventory Level">
                            <padding>
                                <Insets left=" 20" />
                            </padding>
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                        </Label>
                        <TextField fx:id="addPartInvTextField" promptText="Inventory Level" />
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="364.0" spacing="20.0">
                    <children>
                        <Label fx:id="addPartPriceLabel" text="Price/Cost">
                            <padding>
                                <Insets left=" 20" />
                            </padding>
                        </Label>
                        <TextField fx:id="addPartPriceTextField" prefHeight="26.0" prefWidth="149.0" promptText="Price/Cost per Unit">
                     <HBox.margin>
                        <Insets left="30.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0">
                    <children>

                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="148.0" spacing="20.0">
                            <children>
                                <Label fx:id="addPartMinLabel" text="Min" />
                                <TextField fx:id="addPartMinTextField" prefHeight="26.0" prefWidth="91.0" promptText="Minimum" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="175.0" spacing="20.0">
                            <children>
                                <Label fx:id="addPartMaxLabel" text="Max" />
                                <TextField fx:id="addPartMaxTextField" prefHeight="26.0" prefWidth="92.0" promptText="Maximum" />
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets left=" 20" />
                    </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <AnchorPane prefHeight="200.0" prefWidth="325.0">
                            <children>
                                <Label fx:id="addPartMachineCompanyLabel" layoutX="21.0" layoutY="17.0" text="Machine ID" />
                                <TextField fx:id="addPartMachineCompanyTextField" layoutX="137.0" layoutY="12.0" promptText="Machine ID" />
                            </children>
                        </AnchorPane>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Button fx:id="addPartSaveButton" mnemonicParsing="false" onAction="#addPartSaveButtonPushed" text="Save" />
                        <Button fx:id="addPartCancelButton" mnemonicParsing="false" onAction="#addPartCancelButtonPushed" prefWidth="73.0" text="Cancel" />
                    </children>
                    <padding>
                        <Insets left=" 20" />
                    </padding>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>

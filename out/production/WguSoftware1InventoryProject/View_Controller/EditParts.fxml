<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="575.0" prefWidth="560.0" stylesheets="@MainScreenStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.EditPartsController">
    <children>
        <VBox prefHeight="575.0" prefWidth="549.0" spacing="10.0">
            <children>
                <Label fx:id="modifyPartLabel" alignment="CENTER" contentDisplay="CENTER" text="Modify Part">
                    <padding>
                        <Insets bottom="20.0" top="20.0" />
                    </padding>
               <VBox.margin>
                  <Insets left="20.0" />
               </VBox.margin>
                </Label>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="580.0" spacing="20.0">
                    <children>
                        <RadioButton fx:id="modifyPartInhouseRadioButton" mnemonicParsing="false" onAction="#modifyPartInHouseRadio" text="In House" />
                        <RadioButton fx:id="modifyPartOutsourcedRadioButton" mnemonicParsing="false" onAction="#modifyPartOutsourcedRadio" text="Outsource" />
                    </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="20.0" />
               </VBox.margin>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyPartIDLabel" text="Part ID">
                            <padding>
                                <Insets right="50.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="modifyPartIDTextField" disable="true" editable="false" promptText="Auto Gen - ID" />
                    </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyPartNameLabel" text="Part Name">
                            <padding>
                                <Insets right="30.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="modifyPartNameTextField" promptText="Part Name" />
                    </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyPartInvLevelLabel" text="Inventory Level">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="modifyPartInvTextField" promptText="Inventory Level" />
                    </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyPartPriceLabel" text="Price/Cost">
                            <padding>
                                <Insets right="30.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="modifyPartPriceTextField" promptText="Price/Cost per Unit" />
                    </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="175.0" spacing="20.0">
                            <children>
                                <Label fx:id="modifyPartMinLabel" text="Min">
                           </Label>
                                <TextField fx:id="modifyPartMinTextField" prefWidth="100.0" promptText="Minimum" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="175.0" spacing="20.0">
                            <children>
                                <Label fx:id="modifyPartMaxLabel" text="Max" />
                                <TextField fx:id="modifyPartMaxTextField" prefWidth="100.0" promptText="Maximum" />
                            </children>
                        </HBox>
                    </children>
               <padding>
                  <Insets left="20.0" />
               </padding>
               
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <AnchorPane prefHeight="50.0" prefWidth="561.0">
                            <children>
                                <Label fx:id="modifyPartMachineCompanyLabel" layoutX="21.0" layoutY="17.0" text="Label" />
                                <TextField fx:id="modifyPartMachineCompanyTextField" layoutX="137.0" layoutY="12.0" prefHeight="26.0" prefWidth="149.0" promptText="Label" />
                            </children>
                     <padding>
                        <Insets left="20.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                        </AnchorPane>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="580.0" spacing="20.0">
                    <children>
                        <Button fx:id="modifyPartUpdateButton" mnemonicParsing="false" onAction="#modifyPartUpdateButtonPushed" text="Update" />
                        <Button fx:id="modifyPartCancelButton" mnemonicParsing="false" onAction="#modifyPartCancelButtonPushed" text="Cancel" />
                    </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <VBox.margin>
                  <Insets left="20.0" />
               </VBox.margin>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="1000.0" stylesheets="@MainScreenStyle.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View_Controller.EditProductsController">
    <children>
        <Label fx:id="modifyProductTitleLabel" layoutX="22.0" layoutY="61.0" text="Modify Product" />
        <VBox layoutX="500.0" layoutY="61.0" prefHeight="575.0" prefWidth="475.0" spacing="10.0">
            <children>
                <HBox alignment="CENTER" prefHeight="55.0" prefWidth="500.0">
                    <children>
                        <Label fx:id="modifyProductSearchPartLabel" prefHeight="20.0" prefWidth="125.0" text="Existing Parts">
                            <HBox.margin>
                                <Insets right="150.0" />
                            </HBox.margin>
                        </Label>
                        <TextField fx:id="modifyProductSearchPartTextField" prefHeight="26.0" prefWidth="155.0">
                     <HBox.margin>
                        <Insets left="-45.0" />
                     </HBox.margin></TextField>
                        <Button fx:id="modifyProductSearchPartButton" mnemonicParsing="false" onAction="#modifyProductSearchButtonPushed" prefHeight="30.0" prefWidth="80.0" text="Search">
                            <HBox.margin>
                                <Insets left="5.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
                <TableView fx:id="modifyProductAddTableView" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="modifyProductPartIDCol" prefWidth="104.0" text="Part ID" />
                        <TableColumn fx:id="modifyProductPartNameCol" prefWidth="113.0" text="Part Name" />
                        <TableColumn fx:id="modifyProductInvLevelCol" prefWidth="119.0" text="Inventory Level" />
                        <TableColumn fx:id="modifyProductPriceCol" prefWidth="138.0" text="Price Per Unit" />
                    </columns>
                </TableView>
                <HBox alignment="CENTER_RIGHT" prefHeight="55.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="modifyProductAddButton" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#ModifyProductAddButtonPushed" text="Add" />
                    </children>
                </HBox>
                <Label fx:id="modifyProductAssociatedPartsLabel" text="Associated Parts">
                    <padding>
                        <Insets right="100.0" />
                    </padding>
                </Label>
                <TableView fx:id="modifyProductCurrentTableView" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="modifyProductCurrentPartIDCol" prefWidth="104.0" text="Part ID" />
                        <TableColumn fx:id="modifyProductCurrentPartNameCol" prefWidth="113.0" text="Part Name" />
                        <TableColumn fx:id="modifyProductCurrentInvLevelCol" prefWidth="119.0" text="Inventory Level" />
                        <TableColumn fx:id="modifyProductCurrentPriceCol" prefWidth="138.0" text="Price Per Unit" />
                    </columns>
                </TableView>
                <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="200.0">
                    <children>
                        <Button fx:id="ModifyProductDeleteAssociatedPartButton" mnemonicParsing="false" onAction="#ModifyProductDeleteButtonPushed" text="Delete" />
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="200.0" spacing="10.0">
                    <children>
                        <Button fx:id="modifyProductSaveButton" mnemonicParsing="false" onAction="#ModifyProductSaveButtonPushed" text="Update" />
                        <Button fx:id="modifyProductCancelButton" mnemonicParsing="false" onAction="#ModifyProductCancelButtonPushed" text="Cancel" />
                    </children>
                </HBox>
            </children>
        </VBox>
        <VBox alignment="TOP_CENTER" layoutX="30.0" layoutY="182.0" prefHeight="300.0" prefWidth="400.0" spacing="10.0">
            <children>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyProductProductIDLabel" text="Product ID">
                            <padding>
                                <Insets right="25.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="modifyProductIDTextField" disable="true" editable="false" prefHeight="26.0" prefWidth="151.0" promptText="Auto Gen - ID" />
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyProductProductNameLabel" text="Product Name">
                            <padding>
                                <Insets right="5.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="modifyProductNameTextField" prefHeight="26.0" prefWidth="150.0" promptText="Product Name">
                     <HBox.margin>
                        <Insets left="-4.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyProductInvLevelLabel" text="Inventory Level">
                            <HBox.margin>
                                <Insets />
                            </HBox.margin>
                            <font>
                                <Font name="Arial" size="14.0" />
                            </font>
                            <padding>
                                <Insets right="-5.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="modifyProductInvLevelTextField" prefHeight="26.0" prefWidth="151.0" promptText="Inventory Level">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0" spacing="20.0">
                    <children>
                        <Label fx:id="modifyProductPriceLabel" text="Price/Cost">
                            <padding>
                                <Insets right="30.0" />
                            </padding>
                        </Label>
                        <TextField fx:id="modifyProductPriceTextField" prefHeight="26.0" prefWidth="150.0" promptText="Price/Cost per Unit">
                     <HBox.margin>
                        <Insets left="-4.0" />
                     </HBox.margin></TextField>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="600.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="175.0" spacing="20.0">
                            <children>
                                <Label fx:id="modifyProductMinLabel" text="Min" />
                                <TextField fx:id="modifyProductMinTextField" prefWidth="100.0" promptText="Minimum" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="175.0" spacing="20.0">
                            <children>
                                <Label fx:id="modifyProductMaxLabel" text="Max" />
                                <TextField fx:id="modifyProductMaxTextField" prefWidth="100.0" promptText="Maximum" />
                            </children>
                        </HBox>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
